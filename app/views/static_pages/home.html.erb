<% content_for :javascript_includes do %>
 <%= javascript_include_tag "static_pages.js" %>
<% end %>

<div class="row">
  <div >
    <img class="site-logo" src="<%=asset_path "pickup.gif"%>">
  </div>
</div>
<div class="row">
  <div class="home-buttons">
    <a href="<%= new_event_url %>">
      <button
        method="get"
        class="btn btn-primary btn-lg btn-info">
        Host a Pickup
      </button>
    </a>
    <button
      class="btn btn-primary btn-lg btn-warning"
      onclick="getNearby()">
      Join a Pickup
    </button>
  </div>
</div>